{
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaVersion": 6,
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"import"
	],
	"env": {
		"node": true,
		"browser": true
	},
	"globals": {
		"Float32Array": false,
		"Uint16Array": false
	},
	"rules": {
		"semi": "error",
		"eqeqeq": "error",
		"no-extra-semi": "error",
		"semi-spacing": "error",
		"max-len": "error",
		"no-mixed-spaces-and-tabs": "error",
		"indent": [
			"error",
			"tab"
		],
		"no-multi-spaces": "error",
		"no-var": "error",
		"prefer-const": "error",
		"prefer-arrow-callback": "error",
		"array-bracket-spacing": [
			"error",
			"always"
		],
		"block-spacing": [
			"error",
			"always"
		],
		"comma-spacing": "error",
		"eol-last": "error",
		"global-require": "error",
		"no-mixed-requires": "error",
		"comma-dangle": [
			"error",
			"always"
		],
		"key-spacing": "error",
		"object-property-newline": "error",
		"brace-style": "error",
		"guard-for-in": "error",
		"arrow-parens": "error",
		"arrow-spacing": "error",
		"camelcase": "error",
		"curly": [
			"error",
			"all"
		],
		"space-infix-ops": "error",
		"keyword-spacing": "error",
		"import/newline-after-import": "error",
		"import/imports-first": "error",
		"no-console": 0
	}
}